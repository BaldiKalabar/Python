def vetka():
     line(0,0,100,0)
     line(70,0,90,20)
     line(70,0,90,-20)
     line(30,0,50,20)
     line(30,0,50,-20)
def sneginka():
      for i in range(8):
          vetka()
          rotate(radians(45))
           
x = 200
y = 200
def setup():
    size(500,500)
    background(255)
def draw():
       translate(250,250)
       sneginka()
