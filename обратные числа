x= int(input("Введите число: "))
q=(x-x//10*10)
w=(((x-x//100*100-(x-x//10*10)))//10)
e=(((x-x//1000*1000-(x-x//100*100)))//100)
r=(((x-x//10000*10000-(x-x//1000*1000)))//1000)
t=(((x-x//100000*100000-(x-x//10000*10000)))//10000)
y=(((x-x//1000000*1000000-(x-x//100000*100000)))//100000)
u=(((x-x//10000000*10000000-(x-x//1000000*1000000)))//1000000)
i=(((x-x//100000000*100000000-(x-x//10000000*10000000)))//10000000)
o=(((x-x//1000000000*1000000000-(x-x//100000000*100000000)))//100000000)
p=(((x-x//10000000000*10000000000-(x-x//1000000000*1000000000)))//1000000000)
print(q,w,e,r,t,y,u,i,o,p)
