def b():
    print("Что вы хотите изменить?")
    new=input()
    if new=="имя":
        print("Введите новое имя ")
        new=input()
        d["имя"]=new
    if new=="фамилия":
        print("Введите новую фамилию ")
        new=input()
        d["фамилия"]=new
    if new=="отчество":
        print("Введите новое отчество ")
        new=input()
        d["отчество"]=new
    if new=="предмет":
        print("Введите новый предмет ")
        new=input()
        d["предмет"]=new
    if new=="оценка":
        print("Введите новую оценку ")
        new=input()
        d["оценка"]=new
