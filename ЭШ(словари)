d={"имя":"Саша","фамилия":"Иванов","отчество":"Петрович","предмет":"алгебра","оценка":5}
def rs():
    name=input()
    if name=="имя":
         print('Введите имя: ')
         name=input()
         d['имя']=name
    if name=="фамилия":
         print('Введите фамилию: ')
         name=input()
         d['фамилия']=name
    if name=="отчество":
         print('Введите отчество: ')
         name=input()
         d['отчество']=name
